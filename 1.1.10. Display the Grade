a = float(input("subject 1: "))
b = float(input("subject 2: "))
c = float(input("subject 3: "))
d = float(input("subject 4: "))
e = float(input("subject 5: "))

t = (a + b +c + d + e)
M = t/5.0
print("Average Marks: %.2f" %M)

if (100>= M and M>=90):
	print("Grade: A\n")
elif (90>=M and M>=80):
	print("Grade: B\n")
elif (80>=M and M>=70):
	print("Grade: C\n");
elif (70>=M and M>=60):
	print("Grade: D\n")
else:
	print("Grade: F\n")
